{
  "name": "QsrApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.4",
    "@react-native-community/geolocation": "^3.1.0",
    "@react-native-masked-view/masked-view": "^0.3.0",
    "@react-native-picker/picker": "^2.5.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.16",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react-native": "^12.3.2",
    "@types/jest": "^29.5.8",
    "@types/redux-mock-store": "^1.0.6",
    "eslint-plugin-jest": "^27.6.0",
    "formik": "^2.4.5",
    "react": "^18.2.0",
    "react-native": "0.72.6",
    "react-native-animatable": "^1.4.0",
    "react-native-async-storage": "0.0.1",
    "react-native-easy-toast": "^2.3.0",
    "react-native-element-dropdown": "^2.10.0",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-maps": "^1.8.0",
    "react-native-paper": "^5.11.1",
    "react-native-permissions": "^3.10.1",
    "react-native-picker-select": "^9.0.0",
    "react-native-razorpay": "^2.3.0",
    "react-native-reanimated": "^3.5.4",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-simple-toast": "^3.0.2",
    "react-native-vector-icons": "^10.0.1",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@testing-library/jest-native": "^5.4.3",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-native-razorpay": "^2.2.4",
    "@types/react-native-vector-icons": "^6.4.16",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.19.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "^18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "jest": {
    "verbose": true,
    "preset": "react-native",
    "moduleNameMapper": {
      "\\.(png|jpg|ico|jpeg|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/imageMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/setupTest.js",
      "<rootDir>/src/Testhelpers/asyncStorageMock.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(@react-native|react-native|@testing-library/react|react-native-splash-screen|react-native-screens|react-native-onboarding-swiper|react-native-reanimated|d3-(array|format)|react-native-responsive-screen|react-native-vector-icons|@twotalltotems/react-native-otp-input|@react-native-firebase|react-file-drop)/).*/"
    ],
    "testPathIgnorePatterns": [
      "node_modules/",
      "node_modules/lib/"
    ],
    "transform": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileTransformer.js"
    },
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "allowJs": true
        }
      }
    }
  }
}
